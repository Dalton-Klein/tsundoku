{
  "expo": {
    "name": "Tsundoku",
    "slug": "tsundoku",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-square.png",
    "splash": {
      "image": "./assets/splashcustom3.png",
      "resizeMode": "cover",
      "backgroundColor": "#fffbf8"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-logo.png",
        "backgroundColor": "#fffbf8"
      }
    },
    "web": {
      "favicon": "./assets/logo-square.png"
    }
  }
}
